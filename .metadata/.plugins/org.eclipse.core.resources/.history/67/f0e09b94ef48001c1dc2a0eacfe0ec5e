
public class Chapter07 {
	public static void main(String[] args) {
		int distance = 12;
		if (distance <= 5) {
			System.out.println(distance + "とても近いです");
		}else if (distance <= 10) {
			System.out.println(distance + "近いです");
		}else if (distance <= 15) {
			System.out.println(distance + "遠いです");
		}else
			System.out.println(distance + "とても遠いです");
	}

}
